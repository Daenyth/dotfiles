alias rm='rm -i'
alias mv='mv -i'

while read alias; do
    short=$(echo $alias | cut -d' ' -f1 | cut -d. -f2)
    alias $short="git $short"
done < <(git config --global --get-regexp '^alias\.')

alias cdg='cd $(git rev-parse --git-dir)/..'
